<h1> Wszystkie wydarzenia</h1>
<ul>

  <div class="album py-5 bg-light">
    <div class="container product-grid">

      <table>
        <thead>
        <tr>
          <th>Id</th>
          <th>Artysta</th>
          <th>Data</th>
          <th>Cena minimalna</th>
          <th>Cena maksymalna</th>
          <th colspan="3"></th>
        </tr>
        </thead>

        <tbody>
        <% events.each do |event| %>
          <tr>
            <td><%= event.id %></td>
            <td><%= event.artist %></td>
            <td><%= event.event_date %></td>
            <td><%= event.price_low %></td>
            <td><%= event.price_high %></td>
          </tr>
        <% end %>
        </tbody>
      </table>

    </div>
  </div>
</ul>