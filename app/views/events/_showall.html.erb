<h1> Wszystkie wydarzenia</h1>
<div class="container-fluid">
  <div class="row">
    <main role="main" class="col-md-12" style="margin: 0 auto; border: 1px solid">
      <div class="table-responsive">
        <table class="table table-striped table-sm" style="margin-top: 5%">
          <thead>
          <tr>
            <th>Id</th>
            <th>Artysta</th>
            <th>Data</th>
            <th>Cena minimalna</th>
            <th>Cena maksymalna</th>
            <th colspan="3"></th>
          </tr>
          </thead>

          <tbody>
          <% events.each do |event| %>
            <tr>
              <td><%= event.id %></td>
              <td><%= event.artist %></td>
              <td><%= event.event_date %></td>
              <td><%= event.price_low %></td>
              <td><%= event.price_high %></td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </main>

  </div>
</div>